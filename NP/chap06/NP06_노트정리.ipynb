    "from abc import ABC, abstractmethod\n",
    "\n",
    "class Animal    (ABC):\n",
    "    @abstractmethod\n",
    "    def sound(self):\n",
    "        pass\n",
    "\n",
    "class Dog(Animal):  \n",
    "    def sound(self):\n",
    "        return \"멍멍!\"\n",
    "        \n",
    "d = Dog()\n",
    "print(d.sound())  # 출력: 멍멍!"
   ]
  }
